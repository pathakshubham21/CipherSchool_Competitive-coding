 ListNode* middleNode(ListNode* head) {
        ListNode* slow,*fast,*ptr=head;
        int c=0;
        while(ptr!=NULL)
        {
            c++;
            ptr=ptr->next;
        }
        slow=fast=head;
       c=c/2;
        
        while(c--)
        {
            slow=slow->next;
            
        }
 
      
            return slow;
    }
